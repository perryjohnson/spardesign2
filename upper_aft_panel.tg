c make a part for the upper aft panel
block
c specify the node distribution
c   i-list
    1 18;
c   j-list
    1 2 3;
c   k-list (use -1 to use 2D shell elements)
    -1;
c specify the coordinates
c   x-coordinates
    [%bsc/2.0+%bsw] [%chord-%pitchaxis*%chord-%btereinf];
c   y-coordinates
    0.0 0.1 0.2;
c   z-coordinates
    0;

c attach part to guide curves

c attach the top edge
cure
c   region
    0 3 1  c  i-min j-min k-min   (0 picks extreme min)
    0 3 1  c  i-max j-max k-max   (0 picks extreme max)
c   curve_id
    11

c attach the bottom edge
cure
c   region
    0 1 1  c  i-min j-min k-min
    0 1 1  c  i-max j-max k-max
c   curve_id
    20

c attach the left edge
curs
c   region
    1 0 1  c  i-min j-min k-min
    1 0 1  c  i-max j-max k-max
c   curve_id
    26

c interpolate multiple regions as one region
hyr
c   region
    0 0 1  c  i-min j-min k-min
    0 0 1  c  i-max j-max k-max


c make block boundaries ------------------------------------------------------

c make block boundary (slave #11) on left edge
bb
c   region
    1 0 1  c  i-min j-min k-min
    1 0 1  c  i-max j-max k-max
c   interface
    11;

c make block boundary (slave #12) on right edge
bb
c   region
    2 2 1  c  i-min j-min k-min
    2 3 1  c  i-max j-max k-max
c   interface
    12;
